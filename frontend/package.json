{
  "name": "blog-admin-frontend",
  "version": "1.0.0",
  "description": "Vue3 frontend for JonAI-Lab Blog Admin System",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:integration": "vitest --config vitest.integration.config.ts --run",
    "test:integration:watch": "vitest --config vitest.integration.config.ts",
    "test:all": "npm run test && npm run test:integration"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.6.2",
    "chart.js": "^4.5.0",
    "element-plus": "^2.4.4",
    "pinia": "^2.1.7",
    "quill": "^1.3.7",
    "vue": "^3.3.8",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/quill": "^2.0.14",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.0",
    "jsdom": "^25.0.0",
    "typescript": "^5.6.0",
    "vite": "^5.4.0",
    "vitest": "^2.0.0",
    "vue-tsc": "^2.0.0"
  }
}
